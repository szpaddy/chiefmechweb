<?xml version="1.0" encoding="UTF-8"?>
<quartz xmlns="http://www.opensymphony.com/quartz/JobSchedulingData"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.opensymphony.com/quartz/JobSchedulingData http://www.quartz-scheduler.org/xml/job_scheduling_data_1_5.xsd"
	version="1.5">
	<job>
		<job-detail>
			<name>Job_AccessTokenUpdate</name>
			<group>DEFAULT</group>
			<job-class>
				com.chiefmech.weixin.quartz.AccessTokenUpdateJob
			</job-class>
		</job-detail>
		<trigger>
			<cron>
				<name>cron_AccessTokenUpdate</name>
				<group>DEFAULT</group>
				<job-name>Job_AccessTokenUpdate</job-name>
				<job-group>DEFAULT</job-group>
				<cron-expression>0 0/30 * * * ?</cron-expression>
			</cron>
		</trigger>
	</job>
</quartz>